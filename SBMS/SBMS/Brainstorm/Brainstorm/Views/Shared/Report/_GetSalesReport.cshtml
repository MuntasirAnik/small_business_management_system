
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0" style="color:black ;background-color:white">
    <tr>
        <th>Sl</th>
        <th>Code</th>
        <th>Name</th>
        <th>Category</th>
        <th>Sold Qty.</th>
        <th>CP(TK)</th>
        <th>Sales Price(TK)</th>
        <th>Profit(Per Unit)</th>
        <th>Total Profit</th>
   

    </tr>
    @{
        var rep = ViewBag.x;
    }
    @{ int i = 1;}
    @foreach (var p in rep)
    {
        <tr class="Search">

            <td>@(i++)</td>
            <td>@p.Code</td>
            <td>@p.Name</td>
            <td>@p.Category</td>
            <td>@p.SoldQuantity</td>
            <td>@p.CostPrice</td>
            <td>@p.SalesPrice</td>
            <td>@p.Profit</td>
            <td>@p.TotalProfit</td>
        
        </tr>
    }

</table>
